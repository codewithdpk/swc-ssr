{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "dynamicImport": false,
            "target":"es3"
        },
        "externalHelpers": true,
        "transform": {
            "legacyDecorator": true,
            "react": {
                "runtime": "automatic",
                "pragma": "React.createElement",
                "pragmaFrag": "React.Fragment"
            }
        }
    }
}